
public class JavaATM1 {

	public static void main(String[] args) {
		
		
		int originalAmount = 1245;
		int amount = originalAmount;
		
		int n100 = amount /100;
		amount %= 100;
		
		int n50 = amount /50;
		amount %=50;
		
		int n20 = amount /20;
		amount %=20;
		
		
		System.out.println("Notas de R$ 100,00: "+n100);
		System.out.println("Resto:" + amount);

	}

}
